{
  "name": "kerplunk-graphdb",
  "displayName": "Graph Database",
  "description": "Provide a simple Neo4J graph database for use by other plugins",
  "version": "0.0.1",
  "kerplunk": {
    "services": {
      "neo4j": {
        "private": true,
        "image": "neo4j/neo4j:2.2.5",
        "volumes": [
          "/data"
        ],
        "ports": [
          "7474/tcp"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Database"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/db.js",
  "dependencies": {
    "lodash": "^3.7.0",
    "neo4j": "^2.0.0-RC2",
    "request": "^2.60.0",
    "when": "^3.7.3"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1",
    "mocha": "^2.3.2",
    "should": "^7.1.0"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp",
    "test": "NODE_ENV=test mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brianshaler/kerplunk-graphdb.git"
  },
  "license": "MIT"
}
